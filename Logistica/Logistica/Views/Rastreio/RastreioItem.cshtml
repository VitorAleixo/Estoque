
@{
    ViewBag.Title = "RastreioItem";
}

<h2>Produto</h2>
<table class="table table-hover">
    <tr>
        <th scope="col">Produto</th>
        <th scope="col">Status</th>
       
    </tr>
    <tbody>
        <tr class="table-active">
            <td>Produto Aqui</td>
            <td>Status Aqui</td>

        </tr>
        </tbody>
</table>

@using (Html.BeginForm("Rastreio", "Rastreio", FormMethod.Post))
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <td><input type="submit" value="Voltar" class="btn btn-warning" /></td> 
            </div>

        </div>
    </div>

}